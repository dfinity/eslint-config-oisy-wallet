{
  "name": "@dfinity/eslint-config-oisy-wallet",
  "version": "0.2.4",
  "license": "Apache-2.0",
  "description": "Shared ESLint configurations from the Oisy Wallet team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfinity/eslint-config-oisy-wallet.git"
  },
  "bugs": {
    "url": "https://github.com/dfinity/eslint-config-oisy-wallet/issues"
  },
  "main": "index.mjs",
  "files": [
    "README.md",
    "LICENSE",
    "index.mjs",
    "svelte.mjs",
    "vitest.mjs",
    "configs",
    "eslint-local-rules.cjs",
    "rules/**/*.cjs"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.mjs"
    },
    "./svelte": {
      "import": "./svelte.mjs",
      "require": "./svelte.mjs"
    },
    "./vitest": {
      "import": "./vitest.mjs",
      "require": "./vitest.mjs"
    },
    "./eslint-local-rules": {
      "import": "./eslint-local-rules.cjs",
      "require": "./eslint-local-rules.cjs"
    }
  },
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest --run"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "svelte",
    "linting",
    "code-quality",
    "icp",
    "dfinity",
    "oisy",
    "wallet",
    "shared-config",
    "javascript",
    "web-development",
    "best-practices"
  ],
  "peerDependencies": {
    "@dfinity/utils": "*",
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@vitest/eslint-plugin": "^1.6.9",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-svelte": "^3.15.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@typescript-eslint/rule-tester": "^8.56.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=11.5.1 <12.0.0"
  }
}
