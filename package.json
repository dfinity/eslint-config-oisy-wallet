{
  "name": "@dfinity/eslint-config-oisy-wallet",
  "version": "0.0.6",
  "license": "Apache-2.0",
  "description": "Shared ESLint configurations from the Oisy Wallet team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfinity/eslint-config-oisy-wallet.git"
  },
  "bugs": {
    "url": "https://github.com/dfinity/eslint-config-oisy-wallet/issues"
  },
  "main": "index.cjs",
  "files": [
    "README.md",
    "LICENSE",
    "index.cjs",
    "svelte.cjs",
    "eslint-local-rules.cjs",
    "rules/**/*.cjs"
  ],
  "exports": {
    ".": {
      "import": "./index.cjs",
      "require": "./index.cjs"
    },
    "./svelte": {
      "import": "./svelte.cjs",
      "require": "./svelte.cjs"
    },
    "./eslint-local-rules": {
      "import": "./eslint-local-rules.cjs",
      "require": "./eslint-local-rules.cjs"
    }
  },
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier --check .",
    "lint": "eslint -c index.cjs . --ext .cjs",
    "test": "vitest --run"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "svelte",
    "linting",
    "code-quality",
    "icp",
    "dfinity",
    "oisy",
    "wallet",
    "shared-config",
    "javascript",
    "web-development",
    "best-practices"
  ],
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^84.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte": "^2.44.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.3",
    "@typescript-eslint/rule-tester": "^8.7.0",
    "vitest": "^2.1.1"
  }
}
